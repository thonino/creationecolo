{% extends 'base.html.twig' %}

{% block title %}Categorie{% endblock %}

{% block body %}*
<div class="container">
    <h1>Categorie</h1>
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ category.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ category.name }}</td>
            </tr>
        </tbody>
    </table>
    <a class="btn btn-primary mx-1" href="{{ path('app_category_index') }}">Retour</a>
</div>
{% endblock %}
